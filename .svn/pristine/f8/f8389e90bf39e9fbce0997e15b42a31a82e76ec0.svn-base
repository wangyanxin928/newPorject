/*年级table表居中*/
#tableinfo th {
    text-align: center;
}

/*年级table下返回上一页按钮*/
.return {
    height: 40px;
    width: 200px;
    line-height: 40px;
    text-align: center;
    /*color: white;*/
    background: #FFCD70;
    font-size: 18px;
    border-radius: 7px !important;
    margin-bottom: 20px;
    margin-left: 75%;
}

.return a {
    color: white;
    text-decoration: none;
}

.return:hover {
    background: #F0AD4E;
}

.return:hover a {
    color: #eee;
}

